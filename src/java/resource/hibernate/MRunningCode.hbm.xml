<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.smi.travel.datalayer.entity.MRunningCode" table="m_running_code" catalog="smi_uat">
        <id name="id" type="string">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="type" type="string">
            <column name="type" length="10" />
        </property>
        <property name="running" type="java.lang.Integer">
            <column name="running" />
        </property>
    </class>
</hibernate-mapping>
