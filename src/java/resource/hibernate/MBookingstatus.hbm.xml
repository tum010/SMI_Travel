<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.smi.travel.datalayer.entity.MBookingstatus" table="m_bookingstatus" catalog="smitravel">
        <id name="id" type="string">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="name" type="string">
            <column name="name" length="30" not-null="true" />
        </property>
        <set name="masters" table="master" inverse="true" lazy="true" fetch="select">
            <key>
                <column name="Status" />
            </key>
            <one-to-many class="com.smi.travel.datalayer.entity.Master" />
        </set>
    </class>
</hibernate-mapping>
